<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".LoginFragment" android:id="@+id/frameLayout">
    <Button
            android:text="Add Commande"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/addCommande" android:layout_marginTop="48dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="24dp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <Button
            android:text="update It"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/updateCommande" android:layout_marginStart="32dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/addCommande"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nom1" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/updateCommande"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nom2" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/nom1" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ville1" app:layout_constraintStart_toEndOf="@+id/nom1"
            android:layout_marginStart="61dp" app:layout_constraintBaseline_toBaselineOf="@+id/nom1"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ville2"
            app:layout_constraintStart_toEndOf="@+id/nom2"
            android:layout_marginStart="60dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/nom2"/>
    <Button
            android:text="delete It"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/deleteCommande" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/ville2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp"/>
    <Button
            android:text="Afficher Commande par pharmacie"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/commandePharm" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/deleteCommande" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>